{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "import/no-extraneous-dependencies": "off",
    "class-methods-use-this": "off",
    "max-len": "off",
    "consistent-return": "off"
  },
  "settings": {
  }
}
